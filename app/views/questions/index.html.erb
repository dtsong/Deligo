<h1>My questions</h1>


<% @questions.each do |question| %>

        <h3><%= question.question_text %> </h3>  
        <td><%= link_to 'Detailed Breakdown', question %></td>
        <br>
        <h4> Answers </h4>
        <% question.answer_options.each do |answer_option| %>
          <%= answer_option.option %> : <%= Answer.find_count(answer_option.id) %>

          <br>
        <% end %>
        <br>

<% end %>



